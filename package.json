{
  "name": "nest-nuxt-prisma-starter",
  "private": true,
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev": "concurrently -n api,web -c green,blue -k \"pnpm --filter api dev\" \"pnpm --filter web dev\"",
    "build": "pnpm --filter api build && pnpm --filter web build",
    "format": "prettier -w .",
    "lint": "eslint ."
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "eslint": "^9.14.0",
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "@nestjs/mapped-types": "^2.1.0",
    "swiper": "^12.0.3"
  }
}
