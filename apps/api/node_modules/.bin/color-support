#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/elnurayagan/Documents/GitHub/aisanagen/apps/api/node_modules/color-support/node_modules:/Users/elnurayagan/Documents/GitHub/aisanagen/apps/api/node_modules:/Users/elnurayagan/Documents/GitHub/aisanagen/apps/node_modules:/Users/elnurayagan/Documents/GitHub/aisanagen/node_modules:/Users/elnurayagan/Documents/GitHub/node_modules:/Users/elnurayagan/Documents/node_modules:/Users/elnurayagan/node_modules:/Users/node_modules:/node_modules:/Users/elnurayagan/Documents/GitHub/aisanagen/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/elnurayagan/Documents/GitHub/aisanagen/apps/api/node_modules/color-support/node_modules:/Users/elnurayagan/Documents/GitHub/aisanagen/apps/api/node_modules:/Users/elnurayagan/Documents/GitHub/aisanagen/apps/node_modules:/Users/elnurayagan/Documents/GitHub/aisanagen/node_modules:/Users/elnurayagan/Documents/GitHub/node_modules:/Users/elnurayagan/Documents/node_modules:/Users/elnurayagan/node_modules:/Users/node_modules:/node_modules:/Users/elnurayagan/Documents/GitHub/aisanagen/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../color-support/bin.js" "$@"
else
  exec node  "$basedir/../color-support/bin.js" "$@"
fi
